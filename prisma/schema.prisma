generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}
 
model User {
  id        Int      @id @default(autoincrement())
  firstName      String
  lastName String
  email     String   @unique
  image     String
  password String
  createdAt DateTime @default(now())
}
model Meals{
  id Int @id @default(autoincrement())
  imageUrl String
  title String
  category String
  price Int
}
model Corktail{
  id Int @id @default(autoincrement())
  title String
  category String
  price Int
}
model Beer{
  id Int @id @default(autoincrement())
  title String
  category String
  price Int
}